{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],

  "root": true,

  "rules": {
    "no-unused-vars": "error",
    "@typescript-eslint/no-unused-vars": "off",
    "no-use-before-define": "warn",
    "eqeqeq": "error",
    "no-console": "warn"
  }
}
